    "paths": {
        "/dir/customers": {
            "$ref": "dir/customers.json#/paths/~1dir~1customers"
        },
        "/dir/customers/{customerId}": {
            "$ref": "dir/customers.json#/paths/~1dir~1customers~1{customerId}"
        },
        "/dir/test-objects": {
            "$ref": "dir/test-objects.json#/paths/~1dir~1test-objects"
        },
        "/dir/test-objects/{testObjectId}": {
            "$ref": "dir/test-objects.json#/paths/~1dir~1test-objects~1{testObjectId}"
        },
        "/dir/delivery-methods": {
            "$ref": "dir/delivery-methods.json#/paths/~1dir~1delivery-methods"
        },
        "/dir/delivery-methods/{deliveryMethodId}": {
            "$ref": "dir/delivery-methods.json#/paths/~1dir~1delivery-methods~1{deliveryMethodId}"
        },

        "/catalogs/studies": {
            "$ref": "catalogs/studies.json#/paths/~1catalogs~1studies"
        },
        "/catalogs/studies/{studyId}": {
            "$ref": "catalogs/studies.json#/paths/~1catalogs~1studies~1{studyId}"
        }

    },
    "host": "localhost:3000",
    "basePath": "/api",
    "schemes": [
        "http"
    ],
    "consumes": [
        "application/json"
    ],
    "produces": [
        "application/json"
    ]
